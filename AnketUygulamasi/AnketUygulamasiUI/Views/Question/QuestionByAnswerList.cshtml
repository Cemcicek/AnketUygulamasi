@model List<AnketUygulamasiUI.Models.Dtos.ResultAnswerDto>
@{
    ViewData["Title"] = "QuestionByAnswerList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="content" style="padding: 100px 0px 20px 0px;">
    <div class="container-fluid">
        <h4 class="page-title">Cevaplar</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        <table class="table mt-3">
                            <thead>
                                <tr>
@*                                     <th scope="col">#</th> *@
                                    <th scope="col">Anket</th>
                                    <th scope="col">Soru</th>
                                    <th scope="col">Cevap</th>
                                    <th scope="col">Ad</th>
                                    <th scope="col">Soyad</th>
                                    <th scope="col">Email</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
@*                                         <th>@item.AnswerID</th> *@
                                        <th>@item.SurveyName</th>
                                        <td>@item.QuestionName</td>
                                        <td>@item.Answerrs</td>
                                        <td>@item.Firstname</td>
                                        <td>@item.Lastname</td>
                                        <td>@item.Email</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

